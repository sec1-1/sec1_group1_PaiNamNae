meta {
  name: Admin blacklist user สำเร็จ
  type: http
  seq: 1
}

post {
  url: {{base_url}}/api/admin/users/{{driver_id}}/blacklist
  body: none
  auth: inherit
}

tests {
  test("Status code should be 200", () => {
    expect(res.status).to.equal(200);
  });
  
  test("Response message should say blacklist", () => {
    expect(res.body).to.have.property("message");
    expect(res.body.message).to.equal("User blacklisted");
  });
  
  test("Data message should be correct", () => {
    expect(res.body).to.have.property("data");
    expect(res.body.data).to.have.property("message");
    expect(res.body.data.message).to.equal("User blacklisted by admin");
  });
  
}

settings {
  encodeUrl: true
  timeout: 0
}
